import System::Array
import System::Console
import System::Linq::Enumerable

fn main (args: Array String): Unit = do
    for i in Enumerable::Range 1 15 =>
        Console::WriteLine if
            | i % 15 => "FizzBuzz"
            | i % 5 => "Buzz"
            | i % 3 => "Fizz"
            | else => i.ToString()