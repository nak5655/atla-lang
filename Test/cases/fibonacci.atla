import System::Console
import System::Int32

fn main (args: Array String): Unit =
    Console.WriteLine "n>"
    let n = Int32::TryParse (Console.ReadLine ())
	Console.WriteLine (fibonacci n)

fn fibonacci (n: Int): Int =
    match n
        | 0 => 0
        | 1 => 1
        | 2 => 1
        | _ => fibonacci (n - 2) + fibonacci (n - 1)